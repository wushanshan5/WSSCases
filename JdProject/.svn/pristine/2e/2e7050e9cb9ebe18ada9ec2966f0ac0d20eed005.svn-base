/*数字下标部分*/
.number {
    position: absolute;
    left: 50%;
    margin-left: -93px;
    opacity: 0.5;
    bottom: 24px;
    width: 186px;
    padding-left: 5px;
    height: 20px;
    border-radius: 20px;
    line-height: 20px;
    background-color: #000;
}
.number2 {
    position: absolute;
    left: 50%;
    margin-left: -88px;
    bottom: 24px;
}
.number2 i {
    display: inline-block;
    width: 12px;
    height: 12px;
    text-align: center;
    margin: 0 3px;
    background-color: #fff;
    border-radius: 12px;

}
.number2 .i-hover {
    background-color: #DB192A;
}
/*箭头部分*/
.slide-part1-left,.slide-part1-right {
    display: none;
    text-align: center;
    position: absolute;
    top: 50%;
    margin-top: -30px;
    width: 40px;
    height: 60px;
    color: #fff;
    /*使用这个设置背景色，opacity值不会影响到下面去*/
    background-color: rgba(0,0,0,.1);
    font: 25px/60px "Consolas";
    cursor: pointer;
}
.slide-part1-left:hover,.slide-part1-right:hover{
    background-color: rgba(0,0,0,.4);
}
.slide-part1-left {
    left: 0;
}
.slide-part1-right {
    right: 10px;
}
.slide-part2 {
    width: 790px;
    height: 125px;
}
.slide-part2 div {
    float: left;
}
.slide-part2 .slide-part2-left{
    padding-right: 10px;
}

