}
.btn {
    padding: 15px 0 15px 14px;
}
.btn .btn-a {
    border: 2px solid #E01222;
    color: #E01222;
    width: 70px;
    height: 20px;
    font-size: 11px;
    text-align: center;
    line-height: 20px;
    background-color: #fff;
    float: left;
    margin-right: 8px;
}
.btn a:hover {
    color: #fff;
    background-color: #E01222;;
}
/*tab栏*/
.welcome .tab {
    margin: 0 auto;
    width: 162px;
