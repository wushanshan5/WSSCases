/*fixed-nav 部分*/
.fixed-nav {
    position: fixed;
    z-index: 20;
    background-color: #fff;
    left: 0;
    top: 0;
    display: none;
    width: 100%;
    border-bottom: 2px solid #F10214;
}
.fixed-nav .center {
    height: 48px;
    position: relative;
}
.fixed-nav .inp {
    background-color: #EFEFEF;
    width: 495px;
    height: 35px;
    border: 0;
    color: #989898;
    float: left;
    padding-left: 4px;
    margin: 5px 0 0 200px;
}
.fixed-nav img {
    margin-top: 3px;
    float: left;
}

.fixed-nav span {
    left: 793px;
    top: 11px;
}
.fixed-nav .btn {
    height: 35px;
    margin-top: 5px;
}
/*slider部分*/
.slider {
    background-color: #F6F6F6;
}
.slider .center {
    width: 1200px;
    height: 480px;
    padding-bottom: 10px;
}
.slide-left ul {
    position: relative;
    padding-top: 20px;
    float: left;
    font: 13px/27px "Microsoft YaHei";
    background-color: #6E6568;
    width: 190px;
    height: 460px;
    color: #fff;
}
.slide-left li {
    padding-left: 11px;
}
.slide-left ul a {
    color: #fff;
    display: inline-block;
}
.slide-left li:hover {
    background-color: #999395;
}
.slider .slide-left a:hover {
    color: #D01623;
}
.slide-left,.slide-right {
    float: left;
}
/*slide-left-info部分 做为展开项*/
.slide-left ul,.slide-left-info {
    float: left;
}
.slide-left-info {
    position: absolute;
    top: 0;
    left: 190px;
    float: left;
    display: none;
    padding-left: 10px;
    width: 990px;
    height: 530px;
    background-color: #F7F7F7;
    border: 1px solid #D6D6D6;
    border-left: 0;
    border-shadow: 9px;
    border-left: 0;

}
.slide-info-part1,.slide-info-part2 {
    float: left;
}
.slide-info-part1 {
    width: 790px;
    height: 530px;
}
.part1-header {
    padding-top: 21px;
    padding-left: 13px;
}
.part1-header a {
    display: inline-block;
    color: #fff;
    height: 24px;
    padding: 0 9px;
    font-size: 11px;
    margin-right: 10px;
    background-color: #6E6568;
}
.part1-header a:hover {
    background-color: #5C5251;
}
.part1-header a i {
    padding: 0 4px;
    color: #D2DEDA;
    font: 12px/24px "Consolas";
    font-family: "Consolas";
}
.slide-info-part1 .title,.slide-info-part1 .title-info {
    float: left;
}
.title-info,.title {
    float: left;
}
.title-info div {
    display: inline-block;
}
.part1-content .title-info {
    color: #666666;
    font-size: 11px;
    padding: 6px 0;
    line-height: 24px;
    border-bottom: 1px solid #EEEEEE;
    width: 700px;
}
.title-info i {
    color: #EEEEEE;
    padding: 0 10px;
}
.part1-content .title {
    font-weight: 800;
    font-size: 11px;
    width: 80px;
    height: 36px;
    text-align: right;
}
.part1-content .title a,.slide-left  .title-info a {
    color: #666666;
}
.title i {
    font: 15px/36px "Consolas";
    padding: 0 3px 0 8px;
}

.slide-info-part2 {
    padding: 21px 10px;
    width: 179px;
    height: 480px;
}
.slide-info-part2 .img1 {
    width: 83px;
    height: 35px;
}
.slide-info-part2 a {
    width: 83px;
    height: 35px;
    display: block;
    float: left;
    padding: 0 1px 1px 0;
}
.slide-info-part2 .bigpic {
    padding-top: 10px;
}
.slide-info-part2 .bigpic a{
    display: block;
    width: 168px;
    height: 134px;
    padding-bottom: 1px;
}
.slide-info-part2 .bigpic img {
    width: 168px;
    height: 134px;
}


/*slide-right1部分*/
.slide-right {
    float: left;
    padding-left: 10px;
    width: 990px;
    height: 480px;
}
.slide {
    float: left;
}
.slide-part1 {
    position: relative;
    width: 790px;
    height: 340px;
    padding-bottom: 10px;
    padding-right: 10px;
}

/*数字下标部分*/
.number {
    position: absolute;
    left: 50%;
    margin-left: -93px;
    opacity: 0.5;
    bottom: 24px;
    width: 186px;
    padding-left: 5px;
    height: 20px;
    border-radius: 20px;
    line-height: 20px;
    background-color: #000;
}
.number2 {
    position: absolute;
    left: 50%;
    margin-left: -88px;
    bottom: 24px;
}
.number2 i {
    display: inline-block;
    width: 12px;
    height: 12px;
    text-align: center;
    margin: 0 3px;
    background-color: #fff;
    border-radius: 12px;

}
.number2 .i-hover {
    background-color: #DB192A;
}
/*箭头部分*/
.slide-part1-left,.slide-part1-right {
    display: none;
    text-align: center;
    position: absolute;
    top: 50%;
    margin-top: -30px;
    width: 40px;
    height: 60px;
    color: #fff;
    /*使用这个设置背景色，opacity值不会影响到下面去*/
    background-color: rgba(0,0,0,.1);
    font: 25px/60px "Consolas";
    cursor: pointer;
}
.slide-part1-left:hover,.slide-part1-right:hover{
    background-color: rgba(0,0,0,.4);
}
.slide-part1-left {
    left: 0;
}
.slide-part1-right {
    right: 10px;
}
.slide-part2 {
    width: 790px;
    height: 125px;
}
.slide-part2 div {
    float: left;
}
.slide-part2 .slide-part2-left{
    padding-right: 10px;
}

/*welcome部分*/
.welcome {
    float: left;
    width: 190px;
    height: 480px;
    background-color: #fff;
    color: #666767;
}
.welcome .login {
    width: 190px;
    padding-top: 20px;
    height: 90px;
    border-bottom: 1px solid #E6E6E6;
}
.login .photo {
    display: inline-block;
    width: 40px;
    float: left;
    margin-left: 20px;
    border-radius: 40px;
    height: 40px;
    background: url("../images/photo.jpg");
}
.login .login-register {
    float: left;
}
.login-register span {
    display: block;
    width: 100px;
    padding-left: 8px;
    font-size: 11px;
}
.login-register a {
    font-size: 11px;
    padding-left: 8px;
    color: #666767;
}
.login-register a:hover{
