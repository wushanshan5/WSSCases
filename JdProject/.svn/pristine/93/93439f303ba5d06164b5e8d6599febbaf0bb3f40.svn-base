    /*制作左右箭头*/
.kill-left,.kill-right {
    display: none;
    cursor: pointer;
    position: absolute;
    width: 20px;
    height: 50px;
    top: 50%;
    margin-top: -25px;
    text-align: center;
    color: #6A4E4E;
    font: 200 25px/50px "Consolas";
    background-color: #E5E5E5;
}
.kill-left:hover,.kill-right:hover {
    background-color: #E4E4E4;
}
.kill-left {
    left: 7px;
}
.kill-right {
    right: 7px;
}
/*jd-kill-goods 部分*/
.jd-kill-goods {
    position: relative;
}
.jd-kill-goods img {
    width: 100%;
    height: 100%;
}
.jd-kill-goods-dot {
    position: absolute;
    left: 50%;
    margin-left: -16px;
    bottom: 15px;
    width: 35px;
    height: 10px;
}
.jd-kill-goods-dot span {
    display: inline-block;
    width: 10px;
    height: 10px;
    border-radius: 10px;
    background-color: #342A2D;
}
.jd-kill-goods-dot .jd-kill-goods-currentDot {
    background-color: #E01222;

}
.jd-kill .left-top {
    position: absolute;
    left: 0px;
    top: -2px;
    width: 20px;
    height: 20px;
    background: url("../images/icon1.png") -193px -51px;
}
.jd-kill .clock {
    position: absolute;
    top: 10px;
    left: 32px;
    width: 34px;
    height: 40px;
    background: url("../images/icon1.png") 0 -40px;
    margin-right: 20px;
}
.jd-kill-part1 span {
    position: absolute;
    top: 0px;
    left: 87px;
    color: #fff;
    font: 400 27px/55px "华文宋体";
}
.jd-kill-part1 a {
    position: absolute;
    top: 3px;
    left: 210px;
    font: 300 13px/55px "Microsoft YaHei";
    color: #fff;
}
.jd-kill-part1 a .tip {
    position: absolute;
    top: 19px;
    left: 117px;
    margin-left: 5px;
    width: 17px;
    height: 17px;
    background: url("../images/icon1.png") -153px -40px;
}
.jd-kill-part1 .time {
    position: absolute;
    color: #fff;
    font-size: 13px;
    right: 10px;
}
.time i {
    color: #440106;
    font: 800 25px/55px "Consolas";
}
.time i i {
    border-radius: 10px;
    display: inline-block;
    width: 40px;
    text-align: center;
    line-height: 40px;
    height: 40px;
    color: #D90C1C;
    background-color: #440106;
}

