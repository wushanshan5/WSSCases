@font-face {
    font-family: 'iconfont';
    src: url('iconfont.eot');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype'),
        url('iconfont.svg#iconfont') format('svg');
    font-weight: normal;
    font-style: normal;
}

.my-style {
	font-size: 20px;
	line-height: 40rpx;
	font-family: 'iconfont';
    font-weight: normal;
    font-style: normal;
}
.icon-search::before {
	content: "\e66f";
}
/* pages/index/index.wxss */
view {
	box-sizing: border-box;
}
/*  search部分开始  */
.search {
	width: 100%;
	height: 50rpx;
	margin-bottom: 5px;
}
.search-icon {
	position: absolute;
	top: 3px;
	right: 150px;
	color: #111;
}
.search>input {
	position: relative;
	display: inline-block;
	width: 60%;
	font-size: 15px;
	border-radius: 5px;
	padding-left: 10px;
	box-sizing: border-box;
	margin: 0 5px;
	border: 1rpx solid #aaa; 
}
.search>.search-text {
	color: blue;
	font-size: 18px;
	text-decoration: underline;
	float: right;
	width: 30%;
}
/*  search部分结束  */
/*  banner部分start  */
	.banner,.banner>swiper {
		width: 100%;
		height: 400rpx; 
	} 
/*  banner部分end  */

/*  list部分start  */
	.list-item {
		height: 800rpx;
	}
	.list>view {
		display: inline-block;
		width: 50%;
		height: 800rpx;
	}
	.list-item>image {
		width: 90%;
		
	}
	.list  text {
		font: 16px/30px "Microsoft Yahei";
		color: #111;
		display: block;
		width: 150px;
		text-overflow: ellipsis;
		white-space: nowrap;
		overflow: hidden;
		padding-left: 20rpx;
	}
/*  list部分end  */
