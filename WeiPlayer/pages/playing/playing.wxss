/*  fa的引用  */
@font-face {
    font-family: 'iconfont';
    src: url('iconfont.eot');
    font-weight: normal;
    font-style: normal;
}
@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype'),
        url('iconfont.svg#iconfont') format('svg');
    font-weight: normal;
    font-style: normal;
}

.icon-play::before {
	content: "\e632";
}
.icon-pause::before {
	 content: "\e6fc"; 
}
.my-style {
	font-size: 30px;
	line-height: 120rpx;
	font-family: 'iconfont';
    font-weight: normal;
    font-style: normal;
}

/* pages/playing/playing.wxss */
page {
	width: 100%;
	height: 100%;
}
.whole {
	width: 100%;
	height: 100%;
	 overflow: hidden; 
	background-size: 100% 100%;
	background-repeat: no-repeat;
	background-color: rgba(0,0,0,0.9);
	background-origin: border-box
}
.whole>view {
	width: 100%;
	height: 100%;
	 background-color: rgba(255,255,255,0.3); 
}

/*  title部分开始  */
.title {
	text-align: center;
	width: 100%;
	height: 150rpx;
	color: #fff;
}

/*  title部分结束  */
/*  control部分开始  */
	.control {
		position: fixed;
		z-index: 20;
		bottom: 0;
		left: 0;
		width: 100%;
		height: 120rpx;
		background-color: rgba(0,0,0,0.9);
		color: #ffffff;
	}
	.control>progress {
		position: relative;
		top: -7px;
		width: 50%;
		display: inline-block;
		border-radius: 5px;
		margin: 0 auto;
	}
	.play {
		padding: 0 10px;
	}
	.time {
		position: relative;
		top: -5px;
		font-size: 16px;
		/* line-height: 100rpx; */
		padding: 0 10px;
	}
/*  control部分结束  */

/*  lyrics部分开始  */
.lyrics {
	color: #fff;
	font-size: 20px;
	text-align: center;
	line-height: 40px;
}
.lyrics-content {
	width: 80%;
	height: 900rpx;
	color: yellow;
	margin: 0 auto;
	overflow: scroll;
}
/*  lyrics部分结束  */