/*css初始化*/
      body,a,ul,li,input,table,tr,td,p {
          padding: 0;
          margin: 0;
      }
     ul,li {
         list-style: none;
     }
      i {
          font-style: normal;
      }
      .center {
          width: 1187px;
          margin: auto;
      }
      .font {
          color: #999999;
          font-size: 12px;
      }
     .clearfix {
         clear: both;
     }
      .col_red {
          color: #F10215;
      }
      a {
          color: #999999;
          text-decoration: none;
      }
      a:hover {
          color: #c81623;
      }

/*top-banner部分*/
     .top-banner {
         background-color: #FF99B6;
     }
     .top-banner .center {
         height: 80px;
         position: relative;
     }
      .top-banner .close {
          text-align: center;
          display: block;
          width: 20px;
          height: 20px;
          position: absolute;
          top: 5px;
          right: 0px;
          cursor: pointer;
          background-color: #000;
          opacity: 0.3;
          color: white;
          font: 900 18px/20px "Microsoft YaHei";
      }

/*nav 部分*/
      .nav {
          background-color: #E3E4E5;
          border-bottom: 1px solid #DBDBDB;
      }
      .nav .center {
          height: 30px;
      }
       .nav .logo {
           float: left;
           display: inline-block;
           width: 190px;
           height: 170px;
           background: url(../images/logo.jpg);
           text-indent: -5555em;
       }
       .nav-area {
           position: relative;
           margin-left: 10px;
           float: left;
           cursor: pointer;
           text-align: center;
           width: 60px;
           height: 30px;
           z-index: 1;
           line-height: 30px;
       }
       .nav-area .iconfont {
           font-family: 'iconfont';  /* project id 431901 */
           font-size: 15px;
           /*iconfont的颜色是一样的，要设定别的颜色，必须自己设置*/
           color: red;
       }
    .nav-area span {
        position: relative;
        top: -1px;
    }
    .nav-area:hover {
        z-index: 22;
        border: 1px solid #CCCCCC;
        border-bottom: 0;
        background-color: white;
    }


