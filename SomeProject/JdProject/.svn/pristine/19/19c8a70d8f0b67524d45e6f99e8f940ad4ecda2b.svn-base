/*css初始化*/
body,a,ul,li,input,table,tr,td,p {
    padding: 0;
    margin: 0;
}
body {
    background-color: #F6F6F6;
}
ul,li {
    list-style: none;
}
i {
    font-style: normal;
}
.center {
    width: 1187px;
    margin: 0 auto;
}
.font {
    color: #999999;
    font-size: 12px;
}
.clearfix {
    clear: both;
}
.col_red {
    color: #F10215;
}
a {
    color: #999999;
    text-decoration: none;
}
a:hover {
    color: #c81623;
}
/*top-banner部分*/
.top-banner {
    background-color: #FF99B6;
}
.top-banner .center {
    height: 80px;
    position: relative;
}
.top-banner .close {
    text-align: center;
    display: block;
    width: 20px;
    height: 20px;
    position: absolute;
    top: 5px;
    right: 0px;
    cursor: pointer;
    background-color: #000;
    opacity: 0.3;
    color: white;
    font: 900 18px/20px "Microsoft YaHei";
}

/*nav 部分*/
.nav {
    background-color: #E3E4E5;
    border-bottom: 1px solid #DBDBDB;
}
.nav .center {
    height: 30px;
}
.nav .logo {
    float: left;
    display: inline-block;
    width: 190px;
    height: 170px;
    background: url(../images/logo.jpg);
    text-indent: -5555em;
}
.nav-area {
    position: relative;
    margin-left: 10px;
    float: left;
    cursor: pointer;
    text-align: center;
    width: 60px;
    height: 30px;
    z-index: 1;
    line-height: 30px;
}
.nav-area .iconfont {
    font-family: 'iconfont';  /* project id 431901 */
    font-size: 15px;
    /*iconfont的颜色是一样的，要设定别的颜色，必须自己设置*/
    color: red;
}
.nav-area span {
    position: relative;
    top: -1px;
}
.nav-area:hover {
    z-index: 22;
    border: 1px solid #CCCCCC;
    border-bottom: 0;
    background-color: white;
}


/*鼠标放在北京，我的京东，客户服务，网站导航，都有如下一样的代码*/
.list-div-info {
    position: absolute;
    display: none;
    border: 1px solid #CCCCCC;
    border-top: 0;
    z-index: 12;
    background-color: #fff;

}
.list-customerservice:hover,.list-myjd:hover,.list-guide:hover {
    border: 1px solid #ccc;
    padding: 0 2px !important;
    border-bottom: 0;
    background-color: #fff;
}
/*nav-area-places部分*/
.nav-area-places .place-bg {
    background-color: #F10215;
    color: #fff;
}
.nav-area .nav-area-places {
    top: 30px;
    left: -1px;
    width: 300px;
    height: 196px;
    padding: 10px 10px 10px 0;
}
.nav-area-places table tr td {
    width: 60px;
    height: 25px;
    text-align: center;
    font: 11px/25px "Microsoft YaHei";
}
.nav-area-places tr td a {
    display: inline-block;
    width: 40px;
    height: 24px;
    margin: 0 auto;
}
.nav .list {
    width: 655px;
    height: 30px;
    float: right;
}
.list li {
    float: left;
    line-height: 30px;
}
.list .li-div{
    padding: 0 3px;
    display: inline-block;
}
.list li span {
    line-height: 30px;
    padding: 0 5px;
    color: #CCCCCC;
}
/*list list-myjd list-myjd-info部分*/
.list .list-myjd {
    position: relative;
    display: inline-block;
}

.list-myjd-info {
    left: -1px;
}
.list-myjd-info .info1,.list-myjd-info .info2 {
    color: #999999;
    width: 252px;
    line-height: 24px;
    padding: 0 14px;
}
.list-myjd-info .info1 {
    padding-top: 10px;
    height: 82px;
    border-bottom: 1px solid #f1f1f1;
}
.list-myjd-info .info2 {
    padding-top: 5px;
    height: 62px;
}
.list-myjd-info .info1 div,.list-myjd-info .info2 div{
    height: 24px;
    float: left;
    width: 120px;
}
/*list list-customerservice部分*/

.list .list-customerservice {
    position: relative;
}
.list-customerservice .service-info {
    right: -1px;
    width: 155px;
    padding-left: 15px;
    height: 237px;
}
.service-info .info1{
    height: 135px;
    border-bottom: 1px dashed #EEEEEE;
}
.service-info .info2 {
    height: 111px;
}
.service-info .head{
    padding-top: 4px;
    font: 800 11px/26px "Microsoft YaHei";
}
.service-info .info-left,.service-info .info-right {
    font: 11px/22px "Microsoft YaHei";
    float: left;
    width: 77px;
}

/*网页导航部分*/
.list div.list-guide {
    position: relative;
}
.list-guide-info {
    width: 1188px;
    padding: 15px 0;
    top: 30px;
    left: -1040px;
    height: 149px;
    border: 1px solid #CCCCCC;
}
.list-guide-info .head {
    color: #666666;
    padding-top: 4px;
    padding-left: 5px;
    font: 800 11px/15px "Microsoft YaHei";
}
.list-guide-info .info1,.list-guide-info .info2,.list-guide-info .info3,.list-guide-info .info4 {
    float: left;
    padding-left: 19px;
    height: 149px;
    font: 11px/23px "Microsoft YaHei";
    width: 255px;
    border-left: 1px solid #EEEEEE;
}
.list-guide-info table tr td {
    width: 84px;
    height: 24px;
}
.list-guide-info div.info1 {
    width: 340px;
    border-left: 0;
}



/*倒三角的做法*/
.nav  .half {
    display: inline-block;
    position: relative;
    top: 3px;
    height: 9px;
    width: 12px;
    overflow: hidden;
}
.nav  .half i{
    color: #C5C0C7;
    position: absolute;
    top: -15px;
    font: 300 20px/30px "Microsoft YaHei";

}
.list .phonejd{
    position: relative;
}
/*手机京东部分*/
.twoCode-info {
    right: -10px;
    padding: 14px 0 0 5px;
    top: 31px;
    z-index: 12;
    background-color: #fff;
    width: 185px;
    height: 289px;
    border: 1px solid #CFCFCF;
    border-bottom: 3px solid #60575A;
    border-top: 0;
}
.twoCode-info .info1,.twoCode-info .info2,.twoCode-info .info3 {
    height: 100px;
}

.twoCode-info .twoCode-right,.twoCode-info img {
    float: left;
}
.twoCode-info .twoCode-right {
    width: 86px;
    height: 75px;
    padding-left: 4px;
}

.twoCode-info p {
    font: 11px/20px "Microsoft YaHei";
}
.twoCode-info img {
    display: inline-block;
    width: 75px;
    height: 75px;
    border: 1px solid #CCCCCC;
}
.twoCode-info .twoCode-right .p1 {
    color: #666666;
    font-weight: 800;
}
.twoCode-right p a:hover {
    color: #666666;
}
.twoCode-info .p2 {
    color: #F10214;
}
.list .twoCode-info span {
    padding: 0;
    margin: 0;
    display: inline-block;
    width: 25px;
    height: 25px;
}
.twoCode-info .span1 {
    background: url("../images/twoCodeIcon.png") 0 0  no-repeat;
}
.twoCode-info .span2 {
    background: url("../images/twoCodeIcon.png") 0 -26px no-repeat;
}
.twoCode-info .span3 {
    background: url("../images/twoCodeIcon.png") 0 -52px no-repeat;
}
/*二维码部分*/
.list .twoCode {
    position: absolute;
    top: 31px;
    left: -10px;
}
.twoCode .sanjiao {
    position: absolute;
    top: -8px;
    right: 30px;
    width: 0;
    height: 0;
    border: 5px solid #E3E4E5;
    border-bottom-color: #fff;
}
.twoCode .pic {
    width: 66px;
    height: 66px;
    border: 1px solid #CFCFCF;
    background: url("../images/twoCode.png");
}




/*input部分*/
.input {
    background-color: #F6F6F6;
}
.input .center {
    height: 140px;
}
.search {
    position: relative;
    display: inline-block;
    margin: 25px 31px 0px 130px;
}
.search .inp {
    border: 1px solid #F10215;
    width: 495px;
    height: 33px;
    padding-left: 3px;
    color: #989898;
    float: left;

}
.search .btn,.fixed-nav .btn {
    position: relative;
    width: 50px;
    height: 35px;
    background-color: #F10215;
    border: 0;
    font-size: 20px;
    float: left;
    color: #fff;
    cursor: pointer;
}
.btn i {
    position: absolute;
    top: 7px;
    left: 13px;
}
.btn .iconfont {
    font-size: 23px;
}
/*照相机*/
.search span,.fixed-nav span {
    color: #909090;
    position: absolute;
    top: 8px;
    left: 470px;
    font-size: 20px;
}
.search .car-box {
    position: relative;
    width: 190px;
    height: 36px;
    float: left;
    padding-left:32px;
}
.search .car {
    position: relative;
    z-index: 8;
    padding-left:52px;
    width: 136px;
    background-color: #fff;
    height: 34px;
    border: 1px solid #E3E4E5;
    color: #F10215;
    line-height: 33px;
}
.car a {
    font-size: 12px;
    color: #F10215;
    padding-right: 5px;
}
.search .car em {
    font-size: 17px;
    padding-right: 5px;
}
.search .car i {
    position: absolute;
    font: 500 12px/14px "Microsoft YaHei";
    width: 14px;
    height: 14px;
    background-color: #F10215;
    text-align: center;
    color: #fff;
    border-radius: 10px;
    top: 5px;
}
/*car-info部分*/
.car-info {
    z-index: 2;
    top: 35px;
    right: 0;
    width: 308px;
    border-shadow: 2px;
    height: 69px;
    border: 1px solid #CCCCCC;
}
.car-info img,.car .car-word {
    float: left;
}
.car-word {
    color: #999999;
    font-size: 11px;
    line-height: 69px;
}
.car-info img {
    padding: 9px 2px 0 28px;
    width: 57px;
    height: 49px;
}
/*keywords部分*/
.keywords {
    padding-left: 315px;
    width: 700px;
    height: 30px;
}
.input .keywords i {
    padding: 0 5px;
    cursor: pointer;
    float: left;
    font: 500 12px/30px "Microsoft YaHei";
    color: #999999;
}
.keywords i:hover {
    color: #c81623;
}
.input .keywords i.col_red {
    color: #F10215;
}
/*kinds部分*/
.kinds {
    width: 977px;
    padding-top: 2px;
    padding-left: 214px;
    height: 43px;
}
.kinds .kind {
    float: left;
    font: 800 16px/43px "Microsoft YaHei";
}
.kind span {
    padding: 0 13px;
    cursor: pointer;
    color: #555555;
}
.kind span:hover {
    color: #D01623;
}
.kinds .kind i {
    font: 12px/50px "Microsoft YaHei";
    color: #CCCCCC;
}
.kinds .jdpic {
    float: right;
}
