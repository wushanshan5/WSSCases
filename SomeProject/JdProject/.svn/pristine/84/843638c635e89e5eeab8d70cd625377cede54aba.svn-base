 }

/*icon-info部分*/
.icon-info {
    display: none;
    position: relative;
    font-size: 11px;
    width: 190px;
    height: 209px;
}
.icon-info-tab {
    width: 192px;
    height: 24px;
}
.icon-info-tab i {
    cursor: pointer;
    display: block;
    float: left;
    padding-top: 6px;
    border-right: 1px solid #E7E7E7;
    width: 47px;
    text-align: center;
    height: 22px;
}
i#i-last {
    border-right: 0;
}
.tab-i-hover {
    border-top: 2px solid #E01121;
}
.icon-info .close-info {
    cursor: pointer;
    position: absolute;
    right: 2px;
    top: 20px;
    font-size: 25px;
    font-family: "Times New Roman";
    color: #9B8E92;
}
/*icon-info部分的 activity部分*/
.icon-info .activity {
    padding-left: 7px;
    width: 190px;
    height: 160px;
    padding-top: 18px;
}
.activity a {
    color: #666666;
    display: inline-block;
    height: 21px;
    padding: 0 4px;
}
.activity .activity-a-hover {
    background: url("../images/icon-info.png") center;
    color: #fff;
}
.activity .phone label {
    padding-left: 8px;
}
.activity .phone input[type="text"]{
    margin-left: 6px;
    width: 91px;
    height: 20px;
    border: 1px solid #E7E7E7;
    color: #999999;
}
.activity .phone select {
    width: 58px;
    height: 20px;
    font-size: 12px;
    color: #666666;
    border: 1px solid #E7E7E7;
}
.activity .phone span {
    color:#E01222;
}
.phone .phone-box1,.phone .phone-box2 {
    padding: 5px 0;
}
.phone input[type="button"] {
    width: 85px;
    height: 25px;
    color: #fff;
    background-color: #E01222;
    font: 12px/25px "SimSun";
    border: 0;
    margin-left: 10px;
    margin-top: 10px;
    text-align: center;
}
.phone a {
    padding-left: 11px;
    padding-top: 10px;
    display: block;
    font-size: 11px;
    height: 20px;
    color: #527CDB;
}
.phone a img {
    position: relative;
    top: 2px;
    padding-right: 6px;
}
.phone a:hover {
    color: #E01222;
}
/*slide-right2部分 做为展开项*/
    .slide-right2 {
        float: left;
        padding-left: 10px;
        width: 990px;
        height: 530px;
        background-color: #F7F7F7;
        border: 2px solid #F6F6F6;
        border-left: 0;
        background-color: pink;
    }

    /*jd-kill部分*/
    .jd-kill .center{
        position: relative;
        height: 55px;
        background: url("../images/jdkill.png");
    }
    .jd-kill .left-top {
        position: absolute;
        left: 0px;
        top: -2px;
        width: 20px;
        height: 20px;
        background: url("../images/icon1.png") -193px -51px;
    }
    .jd-kill .clock {
        position: absolute;
        top: 10px;
        left: 32px;
        width: 34px;
        height: 40px;
        background: url("../images/icon1.png") 0 -40px;
        margin-right: 20px;
    }
    .jd-kill span {
        position: absolute;
        top: 0px;
        left: 87px;
        color: #fff;
        font: 400 27px/55px "华文宋体";
    }
    .jd-kill a {
        position: absolute;
        top: 3px;
        left: 210px;
        font: 300 13px/55px "Microsoft YaHei";
        color: #fff;
    }
    .jd-kill a .tip {
        position: absolute;
        top: 19px;
        left: 117px;
        margin-left: 5px;
        width: 17px;
        height: 17px;
        background: url("../images/icon1.png") -153px -40px;
    }
    .jd-kill .time {
        position: absolute;
        color: #fff;
        font-size: 13px;
        right: 10px;
    }
    .time i {
        color: #440106;
        font: 800 25px/55px "Consolas";
    }
    .time i i {
        border-radius: 10px;
        display: inline-block;
        width: 40px;
        text-align: center;
        line-height: 40px;
        height: 40px;
        color: #D90C1C;
        background-color: #440106;
    }


