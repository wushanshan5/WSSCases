/*css初始化*/
      body,a,ul,li,input {
          padding: 0;
          margin: 0;
      }
     ul,li {
         list-style: none;
     }
      i {
          font-style: normal;
      }
      .center {
          width: 1187px;
          margin: 0 auto;
      }
      .font {
          color: #999999;
          font-size: 12px;
      }
     .clearfix {
         clear: both;
     }
      .col_red {
          color: #F10215;
      }
      a {
          color: #999999;
          text-decoration: none;
      }
      a:hover {
          color: #c81623;
      }

/*top-banner部分*/
     .top-banner {
         position: relative;
         height: 80px;
         text-align: center;
         background-color: #FF99B6;
     }
      .top-banner .close {
          display: block;
          width: 20px;
          height: 20px;
          position: absolute;
          top: 5px;
          right: 93px;
          cursor: pointer;
          background-color: #000;
          opacity: 0.3;
          color: white;
          font: 900 18px/20px "Microsoft YaHei";
      }

/*nav 部分*/
      .nav {
          background-color: #E3E4E5;
          border-bottom: 1px solid #DBDBDB;
      }
      .nav .center {
          height: 30px;
      }
/*iconfont 的小图标*/
        @font-face {
            font-family: 'iconfont';  /* project id 438573 */
            src: url('//at.alicdn.com/t/font_438573_usgfvf4b6u1munmi.eot');
            src: url('//at.alicdn.com/t/font_438573_usgfvf4b6u1munmi.eot?#iefix') format('embedded-opentype'),
            url('//at.alicdn.com/t/font_438573_usgfvf4b6u1munmi.woff') format('woff'),
            url('//at.alicdn.com/t/font_438573_usgfvf4b6u1munmi.ttf') format('truetype'),
            url('//at.alicdn.com/t/font_438573_usgfvf4b6u1munmi.svg#iconfont') format('svg');
        }
        .iconfont {
            font-family: 'iconfont';  /* project id 431901 */
            font-size: 15px;
            /*iconfont的颜色是一样的，要设定别的颜色，必须自己设置*/
            color: red;
        }
       .nav .logo {
           float: left;
           display: inline-block;
           width: 190px;
           height: 170px;
           background: url(../images/logo.jpg);
           text-indent: -5555em;
       }
       .nav-area {
           margin-left: 10px;
           width: 60px;
           float: left;text-align: center;
           position: relative;
           line-height: 30px;
       }
    .nav-area span {
        position: relative;
        top: -2px;
    }
     .nav-area:hover {
         background-color: white;
     }
      .nav .list {
          width: 706px;
          float: right;
      }
     .nav .list li {
         float: left;

     }
     .nav .list li a{
         display: inline-block;
         line-height: 30px;

     }
     .nav .list li span {
         padding: 0px 11px;
         line-height: 30px;
         color: #CCCCCC;
     }
     .nav  .half {
         display: inline-block;
         position: relative;
         top: 5px;
         height: 10px;
         width: 10px;
         overflow: hidden;
     }
     .nav  .half i{

         position: absolute;
         top: -14px;
         font: 15px/30px Consolas;

     }

/*input部分*/
    .input {
        height: 140px;
        background-color: #F6F6F6;
    }
    .search {
        position: relative;
         display: inline-block;
         margin: 25px 31px 0px 130px;
     }
     .search .inp {
         border: 1px solid #F10215;
         width: 495px;
         height: 33px;
         padding-left: 3px;
         color: #989898;
         float: left;

     }
     .search .btn {
         width: 50px;
         height: 35px;
         background-color: #F10215;
         border: 0;
         font-size: 20px;
         float: left;
         color: #fff;
     }
     /*照相机*/
    .search span {
        color: #909090;
        position: absolute;
        top: 8px;
        left: 470px;
        font-size: 20px;
    }
    .search .car {
        float: left;
        width: 136px;
        background-color: #fff;
        height: 33px;
        border: 1px solid #E3E4E5;
        color: #F10215;
        margin-left: 31px;
        position: relative;
        padding-left:52px;
        line-height: 33px;

    }
    .car a {
        font-size: 12px;
        color: #F10215;
        padding-right: 5px;
    }
    .search .car em {
        font-size: 17px;
        padding-right: 5px;
    }
    .search .car i {
        position: absolute;
        font: 500 12px/14px "Microsoft YaHei";
        width: 14px;
        height: 14px;
        background-color: #F10215;
        text-align: center;
        color: #fff;
        border-radius: 10px;
        top: 5px;
    }
    /*二维码部分*/
    .input .twoCode {
        position: absolute;
        top: 110px;
        right: 84px;
    }
    .twoCode .sanjiao {
        position: absolute;
        top: -8px;
        right: 30px;
        width: 0;
        height: 0;
        border: 5px solid #E3E4E5;
        border-bottom-color: #fff;
    }
    .twoCode .pic {
        width: 66px;
        height: 66px;
        border: 1px solid #CFCFCF;
        background: url("../images/twoCode.png");
    }
    /*keywords部分*/
    .keywords {
        padding-left: 405px;
        width: 700px;
        height: 30px;
    }
    .input .keywords i {
        padding: 0 5px;
        float: left;
        font: 500 11px/30px "Microsoft YaHei";
        color: #999999;
    }
    .input .keywords i.col_red {
        color: #F10215;
    }
    /*kinds部分*/
    .kinds {
        width: 960px;
        padding-top: 2px;
        padding-left: 310px;
        height: 50px;
    }
    .kinds .kind {
        float: left;
        font: 800 16px/50px "Microsoft YaHei";
    }
    .kind span {
        padding: 0 13px;
        cursor: pointer;
        color: #555555;
    }
    .kind span:hover {
        color: #D01623;
    }
    .kinds .kind i {
        font: 12px/50px "Microsoft YaHei";
        color: #CCCCCC;
    }
    .kinds .jdpic {
        float: right;
    }

    /*center-box部分*/
    .center-box {
        width: 3000px;
        height: 480px;
    }

    /*slider部分*/
    .slider {
        background-color: #F6F6F6;
    }
    .slider .center {
        height: 480px;
        padding-bottom: 10px;
    }
    .slider .slide-left {
        color: #fff;
        width: 190px;
        height: 460px;
        padding-top: 20px;
        float: left;
        font: 13px/27px "Microsoft YaHei";
        background-color: #6E6568;
    }
    .slide-left li {
        padding-left: 11px;
    }
    .slider .slide-left a {
        color: #fff;
        display: inline-block;
    }
    .slide-left li:hover {
        background-color: #999395;
    }
    .slider .slide-left a:hover {
        color: #D01623;
    }

    /*slide-right1部分*/
    .slide-right1 {
        float: left;
        padding-left: 10px;
        width: 990px;
        height: 480px;
    }
    .slide {
        float: left;
    }
   .slide-part1 {
       position: relative;
       width: 790px;
       height: 340px;
       padding-bottom: 10px;
       padding-right: 10px;
   }

   /*数字下标部分*/
  .number {
      position: absolute;
      left: 50%;
      margin-left: -93px;
      opacity: 0.5;
      bottom: 24px;
      width: 186px;
      padding-left: 5px;
      height: 20px;
      border-radius: 20px;
      line-height: 20px;
      background-color: #000;
  }
  .number2 {
      position: absolute;
      left: 50%;
      margin-left: -88px;
      bottom: 24px;
  }
  .number2 i {
      display: inline-block;
      width: 12px;
      height: 12px;
      text-align: center;
      margin: 0 3px;
      background-color: #fff;
      border-radius: 12px;

  }
   .number2 .i-hover {
        background-color: #DB192A;
    }
   /*箭头部分*/
   .slide-part1-left,.slide-part1-right {
       display: none;
       text-align: center;
       position: absolute;
       top: 50%;
       margin-top: -30px;
       width: 40px;
       height: 60px;
       color: #fff;
       /*使用这个设置背景色，opacity值不会影响到下面去*/
       background-color: rgba(0,0,0,.1);
       font: 25px/60px "Consolas";
       cursor: pointer;
   }
    .slide-part1-left:hover,.slide-part1-right:hover{
        background-color: rgba(0,0,0,.4);
    }
    .slide-part1-left {
        left: 0;
    }
    .slide-part1-right {
        right: 10px;
    }
    .slide-part2 {
        width: 790px;
        height: 125px;
    }
    .slide-part2 div {
        float: left;
    }
    .slide-part2 .slide-part2-left{
        padding-right: 10px;
    }

    /*welcome部分*/
    .welcome {
        float: left;
        width: 190px;
        height: 480px;
        background-color: #fff;
        color: #666767;
    }
    .welcome .login {
        width: 190px;
        padding-top: 20px;
        height: 90px;
        border-bottom: 1px solid #E6E6E6;
    }
    .login .photo {
        display: inline-block;
        width: 40px;
        float: left;
        margin-left: 20px;
        border-radius: 40px;
        height: 40px;
        background: url("../images/photo.jpg");
    }
    .login .login-register {
        float: left;
    }
    .login-register span {
        display: block;
        width: 100px;
        padding-left: 8px;
        font-size: 11px;
    }
    .login-register a {
        font-size: 11px;
        padding-left: 8px;
        color: #666767;
    }
    .login-register a:hover{
        color: #DB192A;
    }
.btn {
    padding: 15px 0 15px 14px;
}
.btn input {
    border: 2px solid #E01222;
    color: #E01222;
    width: 70px;
    height: 20px;
    font-size: 11px;
    background-color: #fff;
    float: left;
    margin-right: 8px;
}

/*tab栏*/
 .welcome .tab {
     margin: 0 auto;
     width: 162px;
     height: 26px;
     font-size: 11px;
     line-height: 26px;
     border-bottom: 1px solid #E6E6E6;
 }
 .tab .a1,.tab .a2 {
     width: 28px;
     float: left;
     color: #000;
     text-align: center;
     display: block;

 }
 .tab span{
     float: left;
 }
 .tab span {
     padding: 0 12px;
     color: #E6E6E6;
 }
 .tab .more {
     color: #666767;
     display: block;
     float: right;
 }
 #tab-hover {
     border-bottom: 2px solid #DB192B;
 }
/*tab-content 部分*/
.tab-content {
    width: 175px;
    font: 11px/23px "Microsoft YaHei";
    height: 103px;
    padding: 15px 0 15px 15px;
    border-bottom: 1px solid  #E6E6E6;
}
.tab-content i {
    display: block;
}
.content2 {
    display: none;
}

/*welcome 的icon部分*/
.welcome .icon {
    width: 190px;
    height: 209px;
    overflow: hidden;
}
.icon ul{
    width: 200px;
    height: 209px;
}
.icon ul li {
    float: left;
    width: 47px;
    height: 69px;
    border-bottom: 1px solid #E7E7E7;
    border-right: 1px solid #E7E7E7;
}
.icon li span{
    margin: 14px auto 8px auto;
    display: block;
    width: 24px;
    height: 24px;
}
.icon li i {
    display: block;
    width: 47px;
    text-align: center;
    font-size: 11px;
    cursor: pointer;
}
.icon li i:hover {
    color: #D20616;
}
